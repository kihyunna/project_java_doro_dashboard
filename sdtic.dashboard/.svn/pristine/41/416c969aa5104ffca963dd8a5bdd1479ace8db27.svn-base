/* base */
html{font-size:12px;line-height:130%;font-family:"nanum", "Nanum Gothic", "NanumGothic", "나눔고딕", "돋움", "Dotum", Arial, AppleGothic, Sans-Serif;color:#fff;}
input, select, textarea{border:1px solid #a9a9ab;}
input[type=text],input[type=password]{height:28px;}
select{height:30px;font-size:12px;}
input[type=radio],input[type=checkbox], input.rc{margin-top:2px;vertical-align:-3px;}
input[type=password]{width:160px;}
textarea{resize:none;}
a{text-decoration:none;color:#393939;}
a:active,a:hover{text-decoration:underline;color:#000;}
iframe{width:100%;height:100%;background-color:transparent;}


