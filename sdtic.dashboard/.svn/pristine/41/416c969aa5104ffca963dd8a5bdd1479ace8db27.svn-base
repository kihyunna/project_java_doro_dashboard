/*현장설비관제 > 현장설비 지도관제: Info Layer*/
.lyrMInfo{z-index:50;display:none;position:absolute;font-size:12px;color:#000;}
.lyrMInfo .sctLyr{z-index:48;position:relative;padding:25px 22px 22px 22px;background:rgba(255,255,255,0.95);
-webkit-border-radius:0px;
-moz-border-radius:0px;
-o-border-radius:0px;
border-radius:0px;
box-shadow:0px 4px 2px 2px rgba(0,0,0,0.75);
-webkit-box-shadow:0px 4px 2px 2px rgba(0,0,0,0.75);
-moz-box-shadow:0px 4px 2px 2px rgba(0,0,0,0.75);
-o-box-shadow:0px 4px 2px 2px rgba(0,0,0,0.75);
}/*h-shadow(우) v-shadow(하) blur(흐림도) spread(퍼짐도) color 위치(기본 | inset)*/
.lyrMInfo .sctLyr .btnClsL{position:absolute;top:3px;right:3px;cursor:pointer;}
.lyrMInfo .sctLyr .lyrCntt{}
.lyrMInfo .areaFgrArw{z-index:49;position:relative;top:-3px;width:100%;text-align:center;}
.lyrMInfo .fgrArw{width:16px;height:8px;}
.lyrMInfo .fgrArw > polygon{fill:rgba(255,255,255,0.95);}
/*현장설비관제 > 현장설비 지도관제: Info Layer > Table*/
.lyrMInfo table.tblInfo{table-layout:fixed;border-spacing:0;border-top:2px solid #888;}
.lyrMInfo table.tblInfo caption{overflow:hidden;position:absolute;left:-2000%;z-index:-1;font-size:0;line-height:0;}
.lyrMInfo table.tblInfo th, 
.lyrMInfo table.tblInfo td{padding:6px 8px;border-bottom:1px solid #bbb;}
.lyrMInfo table.tblInfo th{background:rgba(100,100,100,0.2);}
.lyrMInfo table.tblInfo td.link{line-height:18px;color:#f47920;}
/*Info Layer: Type02*/
.lyrMInfo.tp02{color:#fff;}
.lyrMInfo.tp02 .sctLyr{background:rgba(0,0,0,0.8);
box-shadow:0px 0px 12px 8px rgba(255,255,255,0.2);
-webkit-box-shadow:0px 0px 12px 8px rgba(255,255,255,0.2);
-moz-box-shadow:0px 0px 12px 8px rgba(255,255,255,0.2);
-o-box-shadow:0px 0px 12px 8px rgba(255,255,255,0.2);
}/*h-shadow(우) v-shadow(하) blur(흐림도) spread(퍼짐도) color 위치(기본 | inset)*/
.lyrMInfo.tp02 .fgrArw > polygon{fill:rgba(0,0,0,0.8);}
.lyrMInfo.tp02 table.tblInfo th{background:rgba(100,100,100,0.75);}
.lyrMInfo.tp02 table.tblInfo td.link a{color:#fff;}


/*현장설비관제 > 현장설비 지도관제: Cover Layout*/
.coverL{z-index:100;display:none;position:absolute;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,0.8);}
.coverL .inner{position:absolute;top:5px;left:5px;bottom:5px;right:5px;border:1px solid rgba(255,255,255,0.2);}
.coverL .inner .innerI{position:relative;width:100%;height:100%;padding:5px;
box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;}
.coverL .inner .innerI .btnClsLM{z-index:90;position:absolute;top:15px;right:15px;cursor:pointer;}
/*현장설비관제 > 현장설비 지도관제: 경고 메세지*/
.coverL .inner .innerI .sctWrn{z-index:80;display:none;position:relative;text-align:center;}
.coverL .inner .innerI .txtWrn{margin-top:10px;font-size:16px;}
/*현장설비관제 > 현장설비 지도관제: 파노라마*/
.coverL .inner .innerI .sctPnrm{z-index:80;display:none;position:relative;width:100%;height:100%;}
.sctPnrm .titPnrm{padding:10px 25px;margin-bottom:1px;font-size:14px;font-weight:bold;color:#fff;background:rgba(68,68,68,0.75);}
.sctPnrm .cnttPnrm{padding:10px 25px;font-size:14px;color:#fff600;background:rgba(51,51,51,0.75);}
.imgPnrm{position:absolute;width:100%;}


