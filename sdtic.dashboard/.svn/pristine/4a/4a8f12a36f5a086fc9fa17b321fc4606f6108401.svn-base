             TEMP1     /* 템프1        */
            ,TEMP2     /* 템프2        */
            ,TEMP3     /* 템프3        */
            ,TEMP4     /* 템프4        */
        FROM TEMPTABLE
    </select>

    <select id="bucCmmnDAO.bucCmmnDataicrrtAjax" parameterClass="bucCmmnVO" resultClass="bucCmmnVO">
