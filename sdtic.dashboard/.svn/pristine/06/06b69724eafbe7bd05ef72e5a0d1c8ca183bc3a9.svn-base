    <insert id="bucInstNasUseDiskDAO.bucEsm241InsertCollectDataAjax" parameterClass="bucCollectEsmVO">
      	<![CDATA[
        /*ESM241 NAS USEDISK 수입데이터 입력 */
        INSERT INTO IOC_SDTIC_BUC_USEDISK_ESM
        (
            ESM_USEDISK_SEQ,
            ESM_GUBUN,        
            HOSTNAME,      
            OBJ_NAME,    
            RSCTYPE_ID,  
            RSC_KEY_NAME,    
            USE_DISK_VALUE,
            COLLECT_DATE,
            CRE_DATE
        )
        VALUES
        (
            ESM_USEDISK_SEQ.NEXTVAL,
            'ESM241',
            #hostName#,
            #obj_name#,
            #rsctype_id#,
            #rsc_key_name#,
            #resultvalue_double#,
            #collect_date#,
            TO_CHAR(sysdate, 'YYYYMMDDhh24mmss')
        )  
       ]]>
     </insert>
     
