function setMdtRatio(data){
	var sHtml = '';
	sHtml += '<table class="tblWgt tp01" summary="수신율별로 금일과 전일의 운행차량 댓수를 보여 준다.">';
	sHtml += '<caption>수신율별 운행차량</caption>';
